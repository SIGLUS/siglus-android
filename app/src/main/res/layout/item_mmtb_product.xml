<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:tools="http://schemas.android.com/tools"
  style="@style/ItemRnr.Container"
  android:layout_width="match_parent"
  android:layout_height="wrap_content">

  <TextView
    android:id="@+id/tv_issued_unit"
    style="@style/ItemRnr.AmountText"
    tools:text="8" />

  <EditText
    android:id="@+id/et_initial_amount"
    style="@style/ItemRnr.AmountText"
    android:background="@null"
    android:enabled="false"
    android:inputType="numberSigned"
    tools:text="9" />

  <TextView
    android:id="@+id/tv_received"
    style="@style/ItemRnr.AmountText"
    tools:text="9" />

  <EditText
    android:id="@+id/et_issued"
    style="@style/ItemRnr.AmountText"
    android:background="@null"
    android:enabled="false"
    android:inputType="number"
    tools:text="9" />

  <EditText
    android:id="@+id/et_adjustment"
    style="@style/ItemRnr.AmountText"
    android:background="@null"
    android:enabled="false"
    android:inputType="numberSigned"
    tools:text="9" />

  <EditText
    android:id="@+id/et_inventory"
    style="@style/ItemRnr.AmountText"
    android:background="@null"
    android:enabled="false"
    android:inputType="number"
    tools:text="9" />

  <TextView
    android:id="@+id/tv_validate"
    style="@style/ItemRnr.AmountText"
    android:enabled="false"
    tools:text="9" />

</LinearLayout>
