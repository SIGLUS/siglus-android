<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- Ignore warnings about field names in R -->
    <Match>
        <Class name="~.*\.R(?:\$.*)?$"/>
        <Bug code="Nm,MS,NP"/>
    </Match>
    <!-- Ignore warnings about code generated by AndroidAnnotations -->
    <Match>
        <Class name="~.*Activity_(?:\$.*)?$"/>
        <Bug code="STYLE,SF"/>
    </Match>
    <!-- Ignore warnings about Views being cast without checks -->
    <Match>
        <Class name="~.*\.ui\..+$"/>
        <Bug code="BC"/>
    </Match>
</FindBugsFilter>